\mode<presentation>
%\definecolor{baylorgreen}{rgb}{.00,.222,.00}
%\definecolor{baylorgold}{rgb}{80,60,0}
\definecolor{baylorgreen}{RGB}{0,72,21}
\definecolor{baylorgold}{RGB}{255,188,25}

\setbeamercolor*{palette primary}{use=structure,fg=baylorgold,bg=baylorgreen}
\setbeamercolor*{palette secondary}{use=structure,fg=baylorgold,bg= baylorgreen}
\setbeamercolor*{palette tertiary}{use=structure,fg=baylorgold,bg= baylorgreen}
\setbeamercolor*{palette quaternary}{fg=baylorgold,bg= baylorgreen}

\setbeamercolor*{sidebar}{use=structure,fg=baylorgold,bg=baylorgreen}

\setbeamercolor*{palette sidebar primary}{use=structure,fg=baylorgold,bg=baylorgreen}
\setbeamercolor*{palette sidebar secondary}{fg=baylorgold,bg=baylorgreen}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=baylorgold,bg=baylorgreen}
\setbeamercolor*{palette sidebar quaternary}{fg=baylorgold,bg=baylorgreen}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}
\setbeamercolor{block title}{use=structure,fg=baylorgold,bg=baylorgreen}
\setbeamercolor{block title alerted}{use=alerted text,fg=baylorgold,bg=baylorgreen}
\setbeamercolor{block title example}{use=example text,fg=baylorgold,bg=baylorgreen}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}



\mode
<all>
